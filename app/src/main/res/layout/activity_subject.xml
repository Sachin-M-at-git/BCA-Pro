<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/SCREEN"
    tools:context=".SubjectsActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <LinearLayout
            style="@style/TEXTTAB"
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <ImageButton
                android:id="@+id/back1"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:background="@drawable/baseline_arrow_back_24"
                android:scaleType="centerCrop"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"/>

            <TextView
                android:id="@+id/textView7"
                android:layout_width="0dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:textColor="@color/white"
                android:text="BCA SUBJECTS"
                android:gravity="center_vertical"
                android:padding="10dp"
                android:textSize="20sp"
                android:textStyle="bold" />
        </LinearLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageButton
                android:id="@+id/rdbms"
                android:layout_width="165dp"
                android:layout_height="165dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="60dp"
                android:layout_marginEnd="15dp"
                android:layout_marginBottom="225dp"
                android:background="@drawable/rdbms"
                app:layout_constraintBottom_toTopOf="@+id/textView3"
                app:layout_constraintEnd_toStartOf="@+id/java"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5" />

            <ImageButton
                android:id="@+id/java"
                android:layout_width="165dp"
                android:layout_height="165dp"
                android:layout_marginTop="60dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="225dp"
                android:background="@drawable/java"
                app:layout_constraintBottom_toTopOf="@+id/textView4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/rdbms"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5" />

            <ImageButton
                android:id="@+id/dip"
                android:layout_width="165dp"
                android:layout_height="165dp"
                android:layout_marginStart="5dp"
                android:layout_marginEnd="15dp"
                android:background="@drawable/dip"
                app:layout_constraintBottom_toTopOf="@+id/statistics"
                app:layout_constraintEnd_toStartOf="@+id/network"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/rdbms"
                app:layout_constraintVertical_bias="0.5" />

            <ImageButton
                android:id="@+id/network"
                android:layout_width="165dp"
                android:layout_height="165dp"
                android:layout_marginEnd="5dp"
                android:background="@drawable/network"
                app:layout_constraintBottom_toTopOf="@+id/softskill"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/dip"
                app:layout_constraintTop_toBottomOf="@+id/java"
                app:layout_constraintVertical_bias="0.5" />

            <ImageButton
                android:id="@+id/statistics"
                android:layout_width="165dp"
                android:layout_height="165dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="21dp"
                android:layout_marginEnd="15dp"
                android:layout_marginBottom="64dp"
                android:background="@drawable/statistics"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/softskill"
                app:layout_constraintHorizontal_bias="0.513"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <ImageButton
                android:id="@+id/softskill"
                android:layout_width="165dp"
                android:layout_height="165dp"
                android:layout_marginTop="21dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="64dp"
                android:background="@drawable/softskills"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.634"
                app:layout_constraintStart_toEndOf="@+id/statistics"
                app:layout_constraintTop_toBottomOf="@+id/textView4" />

            <TextView
                android:id="@+id/textView"
                style="@style/TEXT"
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:text="RELATIONAL DATABASE MANAGEMENT SYSTEM"
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/dip"
                app:layout_constraintEnd_toEndOf="@+id/rdbms"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/rdbms"
                app:layout_constraintTop_toBottomOf="@+id/rdbms"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textView2"
                style="@style/TEXT"
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:text="JAVA PROGRAMMING"
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/network"
                app:layout_constraintEnd_toEndOf="@+id/java"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/java"
                app:layout_constraintTop_toBottomOf="@+id/java"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textView3"
                style="@style/TEXT"
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:text="DIGITAL IMAGE PROCESSING"
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/statistics"
                app:layout_constraintEnd_toEndOf="@+id/dip"
                app:layout_constraintStart_toStartOf="@+id/dip"
                app:layout_constraintTop_toBottomOf="@+id/dip"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textView4"
                style="@style/TEXT"
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:text="COMPUTER NETWORKS &amp; CYBER SECURITY"
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/softskill"
                app:layout_constraintEnd_toEndOf="@+id/network"
                app:layout_constraintStart_toStartOf="@+id/network"
                app:layout_constraintTop_toBottomOf="@+id/network"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textView5"
                style="@style/TEXT"
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:text="COMPUTER ORIENTED STATISTICAL METHODS"
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/statistics"
                app:layout_constraintStart_toStartOf="@+id/statistics"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/statistics"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_editor_absoluteX="54dp" />

            <TextView
                android:id="@+id/textView6"
                style="@style/TEXT"
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:text="SOFT SKILLS-III"
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/softskill"
                app:layout_constraintStart_toStartOf="@+id/softskill"
                app:layout_constraintTop_toBottomOf="@+id/softskill"
                app:layout_constraintVertical_bias="0.0" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </RelativeLayout>

</FrameLayout>